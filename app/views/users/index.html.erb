<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>

<ul>
  <% @users.each do |user| %>
    <li>
      <strong><%= user.name %></strong>
      <% unless current_user == user %>
        <%= link_to 'Send message', new_message_path(to: user.id), class: 'btn btn-default btn-sm' %>
      <% end %>
    </li>
  <% end %>
</ul>

<ul class="users">
  <%= render @users %>
</ul>

<%= will_paginate %>







